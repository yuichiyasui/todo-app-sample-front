{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-unused-vars": "error",
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        },
        "newlines-between": "always"
      }
    ]
  }
}
